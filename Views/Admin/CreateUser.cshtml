@model AppUserData

<h1 class="bg-info text-white">Create User</h1>
<h1 class="bg-info bg-danger text-white text-center">Page is under development</h1>

<a asp-action="Index" class="btn btn-secondary" >Back</a>



<form asp-action="CreateUser" method="post">
    
    <div class="form-group">
        <label asp-for="FirstName"></label>
        <input asp-for="FirstName" class="form-control" />
    </div>
    <div class="form-group">
        <label asp-for="LastName"></label>
        <input asp-for="LastName" class="form-control" />
    </div>
    <div class="form-group">
        <label asp-for="Email"></label>
        <input asp-for="Email" class="form-control" />
    </div>
    <div class="form-group">
        <label asp-for="City"></label>
        <input asp-for="CityName" class="form-control" />
    </div>
    <div class="form-group">
        <label asp-for="Country"></label>
        <input asp-for="CountryName" class="form-control" />
    </div>
    <button type="submit" class="btn btn-primary " disabled>Save</button>
</form>


